{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": { "fetch": false },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-use-before-define": ["error", { "variables": false }],
        "import/prefer-default-export": "off",
        "no-param-reassign": [2, { "props": false }]
    }
}